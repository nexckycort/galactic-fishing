{
  "name": "galactic-fishing",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "tsx scripts/prebuild.ts",
    "build": "rsbuild build",
    "build:rsdoctor": "RSDOCTOR=true pnpm build",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
